{
    "name": "symplify/vendor-patches",
    "description": "Generate vendor patches for packages with single command",
    "license": "MIT",
    "bin": [
        "bin/vendor-patches"
    ],
    "require": {
        "php": ">=8.0",
        "nette/utils": "^3.2",
        "symfony/console": "^5.4|^6.0",
        "symfony/dependency-injection": "^5.4|^6.0",
        "sebastian/diff": "^4.0",
        "cweagans/composer-patches": "^1.7",
        "symplify/composer-json-manipulator": "^10.0.23",
        "symplify/symplify-kernel": "^10.0.23"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\VendorPatches\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\VendorPatches\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "10.1-dev"
        }
    },
    "conflict": {
        "symplify/astral": "<10.0.23",
        "symplify/easy-coding-standard": "<10.0.23",
        "symplify/phpstan-rules": "<10.0.23",
        "symplify/easy-testing": "<10.0.23",
        "symplify/rule-doc-generator-contracts": "<10.0.23",
        "symplify/symfony-php-config": "<10.0.23",
        "symplify/simple-php-doc-parser": "<10.0.23",
        "symplify/php-config-printer": "<10.0.23",
        "symplify/console-color-diff": "<10.0.23",
        "symplify/autowire-array-parameter": "<10.0.23",
        "symplify/markdown-diff": "<10.0.23",
        "symplify/package-builder": "<10.0.23",
        "symplify/amnesia": "<10.0.23",
        "symplify/phpstan-extensions": "<10.0.23",
        "symplify/rule-doc-generator": "<10.0.23",
        "symplify/skipper": "<10.0.23",
        "symplify/smart-file-system": "<10.0.23",
        "symplify/symfony-static-dumper": "<10.0.23",
        "symplify/git-wrapper": "<10.0.23",
        "symplify/monorepo-builder": "<10.0.23",
        "symplify/config-transformer": "<10.0.23",
        "symplify/easy-ci": "<10.0.23",
        "symplify/coding-standard": "<10.0.23",
        "symplify/latte-phpstan-compiler": "<10.0.23",
        "symplify/template-phpstan-compiler": "<10.0.23",
        "symplify/phpstan-latte-rules": "<10.0.23",
        "symplify/easy-parallel": "<10.0.23",
        "symplify/neon-config-dumper": "<10.0.23"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
